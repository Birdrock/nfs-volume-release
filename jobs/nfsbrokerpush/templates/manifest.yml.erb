---
applications:
- name: "<%= p('nfsbrokerpush.app_name') %>"
  buildpacks:
  - binary_buildpack
  routes:
  - route: "<%= p('nfsbrokerpush.app_name') + '.' + p('nfsbrokerpush.app_domain') %>"
  memory: "<%= p('nfsbrokerpush.memory_in_mb') %>M"
  env:
    USERNAME: "<%= p('nfsbrokerpush.username') %>"
    PASSWORD: "<%= p('nfsbrokerpush.password') %>"
